https://practice.geeksforgeeks.org/problems/earthquake-and-the-paint-shop/0


#include<bits/stdc++.h>
#define int long long int 
#define TRY_HARD ios_base::sync_with_stdio(false);  cin.tie(NULL);  cout.tie(NULL);
using namespace std;


signed main()
 {
	TRY_HARD ;
	int t ;
	cin >> t ;
	while ( t-- )
	{
	    int n ;
	    cin >> n ;
	    string s ;
	    cin.ignore() ;
	    map < string , int > m ;
	    for ( int i = 0 ; i < n ; i++ )
	    {
	        getline ( cin , s ) ;
	        m[s]++ ;
	    }
	    for ( auto it = m.begin() ; it != m.end() ; it++ )
	    {
	        cout << it -> first << " " << it -> second << endl ;
	    }
	}
	
}
